	journal = {Journal of Neurology},
	author = {Nelde, Alexander and Klammer, Markus G. and Nolte, Christian H. and Stengl, Helena and Krämer, Michael and Von Rennenberg, Regina and Meisel, Andreas and Scheibe, Franziska and Endres, Matthias and Scheitz, Jan F. and Meisel, Christian},
