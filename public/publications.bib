	journal = {Journal of the American Heart Association},
	author = {Klammer, Markus G. and Reimann, Laura and Richter, Oskar and Lieschke, Simone and Stengl, Helena and Hellwig, Simon and Ganeshan, Ramanan and Schöls, Maximilian and Nelde, Alexander and Meisel, Christian and Villringer, Kersten and Nolte, Christian H. and Endres, Matthias and Scheitz, Jan Friedrich},
	month = oct,
