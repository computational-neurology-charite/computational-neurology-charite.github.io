	file = {Nelde et al. - 2023 - Data lake-driven analytics identify nocturnal non-.pdf:/Users/christianmeisel/Zotero/storage/VX74I5EA/Nelde et al. - 2023 - Data lake-driven analytics identify nocturnal non-.pdf:application/pdf},
